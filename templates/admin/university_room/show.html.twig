{% extends 'base.html.twig' %}

{% block title %}UniversityRoom{% endblock %}

{% block body %}
    <h1>UniversityRoom</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ university_room.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ university_room.name }}</td>
            </tr>
            <tr>
                <th>Stage</th>
                <td>{{ university_room.stage }}</td>
            </tr>
            <tr>
                <th>Capacity</th>
                <td>{{ university_room.capacity }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ university_room.createdAt ? university_room.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>UpdatedAt</th>
                <td>{{ university_room.updatedAt ? university_room.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_university_room_index') }}">back to list</a>

    <a href="{{ path('app_university_room_edit', {'id': university_room.id}) }}">edit</a>

    {{ include('university_room/_delete_form.html.twig') }}
{% endblock %}
