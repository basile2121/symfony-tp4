{% extends 'admin/base_admin.html.twig' %}

{% block title %}Secteur d'activit√©
{% endblock %}

{% block body %}
	<h1>Secteur
		{{sector.name}}</h1>

	<table class="table">
		<tbody>
			<tr>
				<th>Id</th>
				<td>{{ sector.id }}</td>
			</tr>
			<tr>
				<th>Secteur</th>
				<td>{{ sector.name }}</td>
			</tr>
			<tr>
				<th>Description</th>
				<td>{{ sector.description }}</td>
			</tr>
			<tr>
				<th>CreatedAt</th>
				<td>{{ sector.createdAt ? sector.createdAt|date('Y-m-d H:i:s') : '' }}</td>
			</tr>
			<tr>
				<th>UpdatedAt</th>
				<td>{{ sector.updatedAt ? sector.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
			</tr>
		</tbody>
	</table>

	<a class="btn btn-primary" href="{{ path('app_sector_index') }}">Retour</a>

	<a class="btn btn-warning" href="{{ path('app_sector_edit', {'id': sector.id}) }}">Editer</a>

	{{ include('admin/sector/_delete_form.html.twig') }}
{% endblock %}
