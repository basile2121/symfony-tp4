{% extends 'base.html.twig' %}

{% block title %}HighSchool{% endblock %}

{% block body %}
    <h1>HighSchool</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ high_school.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ high_school.name }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ high_school.createdAt ? high_school.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>UpdatedAt</th>
                <td>{{ high_school.updatedAt ? high_school.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_high_school_index') }}">back to list</a>

    <a href="{{ path('app_high_school_edit', {'id': high_school.id}) }}">edit</a>

    {{ include('high_school/_delete_form.html.twig') }}
{% endblock %}
