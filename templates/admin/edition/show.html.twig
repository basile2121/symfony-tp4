{% extends 'admin/base_admin.html.twig' %}

{% block title %}Année
{% endblock %}

{% block body %}
	<h1>Année
		{{ edition.year }}</h1>

	<table class="table">
		<tbody>
			<tr>
				<th>Id</th>
				<td>{{ edition.id }}</td>
			</tr>
			<tr>
				<th>Année</th>
				<td>{{ edition.year }}</td>
			</tr>
			<tr>
				<th>CreatedAt</th>
				<td>{{ edition.createdAt ? edition.createdAt|date('Y-m-d H:i:s') : '' }}</td>
			</tr>
			<tr>
				<th>UpdatedAt</th>
				<td>{{ edition.updatedAt ? edition.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
			</tr>
		</tbody>
	</table>

	<a class="btn btn-primary" href="{{ path('app_edition_index') }}">Retour</a>

	<a class="btn btn-warning" href="{{ path('app_edition_edit', {'id': edition.id}) }}">Editer</a>

	{{ include('admin/edition/_delete_form.html.twig') }}
{% endblock %}
